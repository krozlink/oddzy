[
    {
        "name": "consul",
        "image": "consul:1.2.0",
        "memory": 128,
        "essential": true,
        "portMappings": [
            {
                "containerPort": 8500,
                "hostPort": 8500
            },
            {
                "containerPort": 8600,
                "hostPort": 8600,
                "protocol": "udp"
            }
        ],
        "environment": [
            {"name": "CONSUL_UI_BETA", "value": "true"}
        ]
    }
]