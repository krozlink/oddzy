[
    {
        "image": "675679404987.dkr.ecr.ap-southeast-2.amazonaws.com/oddzy/external/logstash",
        "name": "logstash",
        "memoryReservation": 768,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "oddzy-test",
                "awslogs-region": "ap-southeast-2",
                "awslogs-stream-prefix": "logstash"
            }
        },
        "portMappings": [
            {
                "hostPort": 5000,
                "protocol": "tcp",
                "containerPort": 5000
            }
        ]
    }
]