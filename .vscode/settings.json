{
    "go.buildOnSave": "package",
    "go.vetOnSave": "off",
    "todo-tree.flat": false,
    "todo-tree.globs": [
        "!**/vendor/**"
    ],
    "protoc": {
        "options": [
            "--proto_path=${workspaceRoot}/**/proto",
            "--proto_path=${env.GOPATH}/src"
        ]
    },
    "files.exclude": {
        "**/vendor/**": true,
        "**/Gopkg.*": true,
    },
    "terraform.codelens.enabled": false,

    "docker.truncateLongRegistryPaths": false,
    "docker-explorer.showAzureRegistries": false,
    "docker-explorer.showDockerHubTreeView": false,
    "docker-explorer.showSuggestedDockerImages": false,
    "docker-explorer.showDockerImages": false,
}